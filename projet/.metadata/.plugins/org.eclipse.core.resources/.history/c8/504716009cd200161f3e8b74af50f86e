package VocabularySystem;
import java.io.File;
import java.io.FileNotFoundException;
import java.util.HashMap;
import java.util.Scanner;

public class FileTopicReader implements TopicReader {

	private HashMap<String, String> topic;
	private Scanner sc;
	private File topicFile;
	
	public FileTopicReader() {
		this.topic = new HashMap<String,String>();
		this.topicFile = null;
	}
	
	@Override
	public HashMap<String, String> extractTopic(File topicFile) throws FileNotFoundException {
		this.topicFile = topicFile;
		sc = new Scanner(topicFile);
		return null;
	}
		
}
